{
  "name": "aeroscore",
  "version": "0.2.2",
  "description": "Gliding Competition Scoring",
  "main": "index.js",
  "repository": "https://github.com/Turbo87/aeroscore",
  "author": "Tobias Bieniek <tobias.bieniek@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "changelog": "lerna-changelog",
    "lint": "tslint --project . --format stylish",
    "test": "jest",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "@turf/bearing": "^6.0.1",
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/center": "^6.0.1",
    "@turf/circle": "^6.0.1",
    "@turf/distance": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@turf/sector": "^5.1.5",
    "@turf/union": "^6.0.3",
    "aprs-parser": "^1.0.4",
    "cheap-ruler": "^2.5.1",
    "flight-recorder-manufacturers": "^1.1.0",
    "igc-filename-parser": "^0.1.1",
    "igc-parser": "^0.5.0",
    "monotone-convex-hull-2d": "^1.0.1",
    "tiny-emitter": "^2.0.2",
    "xml-js": "^1.6.7"
  },
  "devDependencies": {
    "@types/d3-dsv": "^1.0.33",
    "@types/geojson": "^7946.0.4",
    "@types/jest": "^25.1.1",
    "@types/node": "^6.0.83",
    "cli-table3": "^0.5.1",
    "d3-dsv": "^1.0.8",
    "fetch-ponyfill": "^6.0.2",
    "jest": "^24.1.0",
    "lerna-changelog": "^0.8.2",
    "log-update": "^3.0.0",
    "opn": "^6.0.0",
    "temp-write": "^4.0.0",
    "ts-jest": "^24.0.0",
    "tslint": "^6.1.3",
    "typescript": "^3.0.1",
    "ws": "^7.0.0"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "snapshotSerializers": [
      "<rootDir>/tests/coordinate-serializer.js",
      "<rootDir>/tests/timestamp-serializer.js",
      "<rootDir>/tests/float-serializer.js"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts|js)$",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
