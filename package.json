{
  "name": "aeroscore",
  "version": "0.0.0",
  "description": "Gliding Competition Scoring",
  "main": "index.js",
  "repository": "https://github.com/Turbo87/aeroscore",
  "author": "Tobias Bieniek <tobias.bieniek@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project . --format stylish",
    "test": "jest"
  },
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "@turf/bearing": "^4.5.2",
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/center": "^6.0.1",
    "@turf/circle": "^4.5.2",
    "@turf/helpers": "^6.1.4",
    "@turf/sector": "^4.5.2",
    "@turf/union": "^4.5.2",
    "aprs-parser": "^1.0.4",
    "cheap-ruler": "^2.5.1",
    "flight-recorder-manufacturers": "^1.1.0",
    "igc-filename-parser": "^0.1.1",
    "igc-parser": "^0.4.0",
    "monotone-convex-hull-2d": "^1.0.1",
    "tiny-emitter": "^2.0.2",
    "xml-js": "^1.6.4"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.3",
    "@types/jest": "^22.2.3",
    "@types/node": "^6.0.83",
    "jest": "^23.1.0",
    "log-update": "^2.3.0",
    "opn": "^5.3.0",
    "temp-write": "^3.4.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.8.4",
    "ws": "^5.2.0"
  },
  "engines": {
    "node": ">=6"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "snapshotSerializers": [
      "<rootDir>/tests/coordinate-serializer.js",
      "<rootDir>/tests/timestamp-serializer.js",
      "<rootDir>/tests/float-serializer.js"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts|js)$",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
